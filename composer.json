{
  "name": "somin-team/somin-api-sdk",
  "description": "SoMin API SDK is a PHP library for making calls to SoMin API.",
  "keywords": ["somin", "social", "multimedia analytics", "api", "sdk"],
  "type": "library",
  "homepage": "https://github.com/bbridge-team/somin-sdk-php",
  "license": "Apache License 2.0",
  "authors": [
    {
      "name": "Lepikhin Kirill",
      "homepage": "https://github.com/casskir"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.7",
    "squizlabs/php_codesniffer": "~3.0",
    "phpmd/phpmd": "~2.6"
  },
  "autoload": {
    "psr-4": {
      "SoMin\\": "src",
      "SoMin\\Test\\": "tests"
    }
  }
}